{
  "devDependencies": {
    "@types/node": "^24.3.1",
    "typescript": "^5.9.2",
    "vscode": "^1.1.37"
  },
  "name": "vs-links",
  "version": "4.2.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "icon": "images/icon.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "publisher": "Steve-RimJobs",
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "VS Links",
      "properties": {
        "vs-links.branchUrlPattern": {
          "type": "string",
          "default": "https://github.com/${project}/tree/${branch}",
          "description": "URL pattern for the remote branch link."
        },
        "vs-links.prUrlPattern": {
          "type": "string",
          "default": "https://github.com/${project}/compare/${branch}...main",
          "description": "URL pattern creating a PR from the branch link to main."
        }
      }
    }
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/feesh42/vs-links.git"
  }
}
